{
  "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      // "module": "es2022",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "Node",
      "baseUrl": "src",
